__lua__
// map
m = {
 sx=0,
 sw=128
}

ground_y=96

// player
p = {
 x=16,
 y=ground_y,
 w=2,
 h=2,
 dy=0,
 mode="start",
 wait=0,
 score=0,
 walk_sfx=false
}

// game
g = {
 level=0
}

frame=0

function _init()
 music(2)
end

dx=0

function _update60()
 frame=(frame+1)%60
 
 p.wait=max(0,p.wait-1)
 
 // main game loop
 if p.mode=="run" or p.mode=="lie" then 
  if p.mode=="lie" then
   dx=max(0,dx*0.9)
   if p.wait==0 then
    p.mode="wait"
    p.wait=60
    sfx(25)
   end
  else
   dx=1+0.125*g.level
  end
  m.sx=m.sx+dx
  
  // next level..!
  if m.sx>=128 then
   m.sx=0
   
   if p.mode=="run" then
    g.level+=1
   end
  end
 
  if p.y==ground_y then
 	 if p.mode=="run" and btn()>0 then
 	  p.dy=-4 // jump height
 	  sfx(0,1)
 	 end
  else
   // gravity
 	 p.dy+=0.25
  end
 
  p.y+=p.dy
  if (p.y>ground_y) then
   p.y=ground_y
   p.dy=0
   sfx(1,1)
   // land sfx
  end
 
  if p.mode=="run" then
   // detect hit
   local b = get_baddie()
 
   if b!=nil and rsect(p,b) then
    sfx(6,0)
    // die sfx
    p.mode="lie"
    p.wait=120
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
    pts_add(p.x,p.y)
   end
   
   p.score+=1
   if p.score%500==0 then
    sfx(11,2)
   end
  end
 end
 
 //
 if p.mode=="start" then
  if btn()>0 then
   p.mode="run"
   music(-1)
   // stop intro music
   sfx(2,3)
   // music
  end
 end
 
 if p.mode=="run" then
  if p.y==ground_y and p.walk_sfx==false then
   sfx(7,0)
   p.walk_sfx=true
  else
   if p.y<ground_y and p.walk_sfx==true then
    sfx(-1,0)
    p.walk_sfx=false
   end
  end
 end
 
 
 
 pts_update()
end

function get_baddie()
 local l = levels[g.level%20]
 
 if l != nil and l>0 then
  local h=ground_y
  if l==3 then
   h=ground_y-20
  end
  // baddie
  return {
   sp=baddie[l],
   x=116-m.sx,
   y=h,
   h=2,
   w=2
  }
 end
 return nil
end

function _draw() 
 cls()
 
 if p.mode=="run" or p.mode=="lie" then
 
  // ground
  map(0,0,-m.sx)
  
  pts_draw()
  
  // player
  
  if p.mode=="run" then
   local an=flr(frame/8)%2+1
   local ani={8,38}
   spr(ani[an],p.x,p.y,2,2)
  else
   spr(0x24,p.x,p.y,2,2)
  end
  
  local b = get_baddie()
  if b!=nil then
   spr(b.sp,
     b.x,
     b.y,
     2,
     2)
  end
  
  print("score: "..p.score)
  //print("dx:"..dx)
 end
 
 if p.mode=="start" then
  pts_add2(rnd(128),16)
  pts_draw()
  map(24,48,32,32,8,8)
  print("press ❎ to start",32,12)
 end
 
 if p.mode=="dead" or p.mode=="wait" then
  map(5,46,0,16,16,16)
 	print("you very dead",28,8,7)
 	print("score: "..p.score,34,0,7)
 	if p.wait<=0 then
 	 print("press ❎ to retry",24,64,7)
 	 p.mode="dead"
 	 p.wait=0
 	 // end screen
 	 if btn()>0 then
 	  // reset game
 		 p.mode="run"
 		 p.wait=0
 		 p.score=0
 		 p.dy=0
 		 g.level=0
 		 m.sx=0
 		 sfx(2,3)
 	 end
 	end
 end
 
 //print("p.wait:"..p.wait)
 //print("mode:"..p.mode)
// print("p.walk_sfx"..p.walk_sfx)
end
