// point-in-rect
// p={x,y,w,h}
function psect(x,y,r) 
 if r==nil then
  return false
 end
 
	if   x>=r.x 
	 and x<r.x+r.w*8
	 and y>=r.y
	 and y<r.y+r.h*8 then
	 return true
	else
		return false
	end
end

// rect overlaps rect
// r={x,y,w,h} in px
function rsect(r1,r2) 
 if r1==nil or r2==nil then
 	return false
 end
 
 local inset=4
 
 // true if any corner of 
 // 1st rect in second one
 return psect(
   r1.x+inset,
   r1.y+inset,r2)
  
  or psect(
   r1.x+r1.w*8-inset,
   r1.y+inset,
   r2)
   
  or psect(
   r1.x+inset,
   r1.y+r1.h*8-inset,
   r2)
   
  or psect(
   r1.x+r1.w*8-inset,
   r1.y+r1.h*8-inset,
   r2)
end
