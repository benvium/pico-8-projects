// attacks

cat_attack={
 {},
 {0,-1,0xc1},
 {-1,-1,0xc0},
 {-1,0,0xd0},
 {-1,1,0xe0},
 {0,1,0xe1},
 {1,1,0xe2},
 {1,0,0xd2},
 {1,-1,0xc2}
}

cat_attack_frame=0


function update_cat_attack()
  if cat_attack_frame==0 then
   return
  end
  cat_attack_frame+=1
  if cat_attack_frame==10 then
   cat_attack_frame=0
  end
end

function draw_cat_attack()
  if cat_attack_frame==0 then
   return
  end
  local a=cat_attack[cat_attack_frame]
  
  local ax=x-sx+a[1]*8
  local ay=y-sy+a[2]*8
  spr(
  a[3], 
  ax,
  ay
  )
  // todo work out if npcs there
  hit_npc_at(ax+sx,ay+sy)
end

