// npcs

npcs={
 {x=5*8,y=2*8,type=1,dx=0,dy=1},
 {x=12*8,y=5*8,type=19,dx=1,dy=1},
 {x=4*8,y=9*8,type=2,dx=0,dy=0},
 // temple
 {x=71*8,y=9*8,type=19,dx=0,dy=0}
}

function hit_npc_at(x,y)
 for k,npc in pairs(npcs) do
	 if npc.x>=x and npc.x<x+8 
	 and npc.y>=y and npc.y<y+8 then
	  //npc.x=-100
	  if (x>npc.x) then npc.dx=3 end
	  if (x<npc.x) then npc.dx=3 end
	  if (y>npc.y) then npc.dy=3 end
	  if (y<npc.y) then npc.dy=3 end
	  sfx(6,2)
	 end
	 
	end
end

function update_npcs() 
	for k,npc in pairs(npcs) do
	 npc.x=npc.x+npc.dx
	 npc.y=npc.y+npc.dy
	 
	 if flr(rnd(30))==1 then
	 	npc.dx = flr(rnd(3))-1
	 	npc.dy = flr(rnd(3))-1
	 end
	 	
	 local f = map_flag_get(npc.x,npc.y,npc.dx,npc.dy)
	 if (f.fx==1) then npc.dx=0 end
	 if (f.fy==1) then npc.dy=0 end
	end
end
