//sx=(52-8)*8
//sy=(6-8)*8
sx=0
sy=0
frame=0
on_stone=0

// collision detection with map
function map_flag_get(x,y,dx,dy)
  // new map x
  local nx = ((x+4)+dx*4)/8
  local ny = ((y+4)+dy*4)/8
 
  // flag on map tile to move to
  local fx = fget(mget(nx,(y+4)/8))
  local fy = fget(mget((x+4)/8,ny))
  
  return {fx=fx,fy=fy}
end

function _update()
  if (btn(0)) then dx=-1 end
  if (btn(1)) then dx=1 end
  if (btn(2)) then dy=-1 end
  if (btn(3)) then dy=1 end
  
  // attack
  if (btn(4)) then
   if cat_attack_frame==0 then 
   	sfx(8)
   	cat_attack_frame=1
   end 
  end
  
  // new map x
  local nx = ((x+4)+dx*4)/8
  local ny = ((y+4)+dy*4)/8
 
  // flag on map tile to move to
  local fx = fget(mget(nx,(y+4)/8))
  local fy = fget(mget((x+4)/8,ny))
  
  // check solid tile in new x pos
  if fx==1 then
   dx=0
  end
  
  // check in new y pos
  if fy==1 then
   dy=0
  end
  
  // check slow tile in new x pos
  if fx==2 or fy==2 then
   dy=dy/2
   dx=dx/2
   sfx(6)
  end
  
  // check foot-tap tile
  if (fx==4 or fy==4) and (dx!=0 or dy!=0) then
   if on_stone==0 then
    sfx(7,1)
    on_stone=1
   end	
  else
   if on_stone then
    on_stone=0
    sfx(-1,1)
   end
  end
  
  x=x+dx
  y=y+dy

  // if close to edge, scroll  
  if x-sx>96 or x-sx<16 then
   sx+=dx
  end
  
  if y-sy>96 or y-sy<16 then
   sy+=dy
  end
  
  // if collect coin move
  // coin off-screen
  for k, coin in pairs(coins) do
  	if (flr(nx)==coin.x and flr(ny)==coin.y) then
  	  sfx(9)
  	  coin.x = -10
  	  coin.y = -10
  	  coin_score+=1
  	end
  end
  
  // if hit door - move
  for k, door in pairs(doors) do
   if (flr(nx)==door.ix and flr(ny)==door.iy) then
   	sx=(door.ox-8)*8
   	sy=(door.oy-8)*8
   	x=door.ox*8
   	y=door.oy*8
   	sfx(10)
   end
  end 
  
  update_npcs()
  
  update_cat_attack()
  
end

function _draw()
  cls()
  map(0,0,-sx,-sy)
  
  // animate walk
  if (dx!=0 or dy!=0) then
    frame=(frame+1)%10
  end
		spr(4+(frame/5),x-sx,y-sy)
		dx=0
		dy=0
		// ^ stop movement
		
		coin_frame = (coin_frame+1)%6
		for k,coin in pairs(coins) do
		  spr(coin_anim[coin_frame+1],coin.x*8-sx, coin.y*8-sy)
		end
		// draw score
		print(coin_score,3,3,10)
		
		// npcs
		for k,npc in pairs(npcs) do
		 spr(npc.type,npc.x-sx,npc.y-sy)
		end
		
		// attach
		draw_cat_attack()
end
